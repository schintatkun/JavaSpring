<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8" %>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Demo JSP</title>
</head>
<body>
	<h1>Created JSP</h1>
	<%!
		public int add(int a, int b){
		return a+b;
	}
	%>
	<%
		int i =2;
		int j =4;
	%>
	
	<h3><%= add(i, j)%></h3>
	<%for(int i=0;i<8;i++) { %>
		<h1><%=i%></h1>
	<%} %>
	
</body>
</html>